{
  "name": "eph/prh",
  "description": "PRH API",
  "type": "library",
  "version": "1.0.0",
  "autoload": {
	"psr-4" : {
	  "Eph\\Prh\\" : "src/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "Eph\\Prh\\Tests\\": "tests/"
	}
  },
  "require": {
	"php": ">=7.3",
	"ext-json": "*",
    "guzzlehttp/guzzle": "^7.2",
    "psr/log": "^1.1"
  },
  "suggest": {
	"monolog/monolog": "Allows logging of errors"
  },
  "require-dev": {
	"friendsofphp/php-cs-fixer": "~2.0",
	"phpstan/phpstan": "~0.12",
	"phpstan/phpstan-deprecation-rules": "~0.12",
	"phpstan/phpstan-strict-rules": "~0.12",
	"phpunit/phpunit": "~8.0"
  },
  "prefer-stable": true
}
